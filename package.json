{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@types/es6-shim": "^0.31.42",
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "mongoose": "^7.4.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
