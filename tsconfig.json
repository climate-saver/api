{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitThis": true,
    "paths": {
      "@modelInterfaces": ["./modelInterfaces"]
    },
    "lib": ["es2017", "dom", "es2017.object"],
    "strict": true,
    "skipLibCheck": true
  }
}
